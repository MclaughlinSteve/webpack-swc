{
  "name": "@htc/webpack-swc",
  "version": "1.0.0",
  "description": "Example on how to configure an ESM component library with webpack and swc",
  "main": "dist/index.js",
  "author": "Drew DeCarme",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "module": "dist/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.cjs",
    "prestorybook": "cp src/root.css ./.storybook/public",
    "storybook": "yarn prestorybook && start-storybook -p 6006 --static-dir ./.storybook/public",
    "build-storybook": "yarn prestorybook && build-storybook --static-dir ./.storybook/public"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@swc/core": "1.2.103",
    "@swc/jest": "^0.2.5",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "css-loader": "^6.5.0",
    "eslint": "^8.1.0",
    "eslint-plugin-react": "^7.26.1",
    "mini-css-extract-plugin": "^2.4.3",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.1.15",
    "typescript": "^4.4.4",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "clsx": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
